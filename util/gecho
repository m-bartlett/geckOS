#!/bin/bash
G="\033[0;32m"
R="\033[0;31;42m"
Y="\033[0;33m"
C="\033[0;m"
echo -e "
      $G/\\     
 $Y. .$G $Y@$R)($Y@. .$G  ┏━━━━━━━━━━━━━━━━┓
$Y.${G}_\\| \\\\$R--$G/|/_$Y.$G ┃ ┌┐┌┐┌┐│ ╔═╗╔═╕ ┃
   \\//$R--$G\\\\/    ┃ └┤├┘│ │╱║ ║╚═╗ ┃
    \`|$R--$G|\`  $Y.$G ┃ └┘└┘└┘│╲╚═╝╘═╝ ┃
 $Y.$G   |$R--$G| ,/_$G ┗━━━━━━━━━━━━━━━━┛
$Y.${G}_\\, |$R--$G|◜/\\\\$Y.\`
 .$G/\\◝|$R--$G;/         
    \\;$R--$G\\             
      '.$R/$G\\                
 .-.    \\\\$R-$G|             
 \`  '.__//                    
      \`\"\`                   
$C"
